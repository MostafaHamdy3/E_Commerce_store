{
  "expo": {
    "name": "store_payIn",
    "slug": "store_payIn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "react-native-bootsplash",
        {
          "assetsDir": "assets/bootsplash",
          "android": {
            "parentTheme": "TransparentStatus",
            "darkContentBarsStyle": true
          }
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(Store_PayIn) to use Face ID."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "The Store_PayIn app uses Face ID for authentication."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mostafa7amdy.store_payIn"
    },
    "web": {
      "favicon": "./src/assets/logo.png"
    }
  }
}
