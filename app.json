{
  "expo": {
    "name": "store_payIn",
    "slug": "e-commerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "react-native-bootsplash",
        {
          "assetsDir": "assets/bootsplash",
          "android": {
            "parentTheme": "TransparentStatus",
            "darkContentBarsStyle": true
          }
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(Store_PayIn) to use Face ID."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "The Store_PayIn app uses Face ID for authentication."
      },
      "bundleIdentifier": "com.mostafa7amdy.store-payIn"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mostafa7amdy.store_payIn",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./src/assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "193330e6-3c4b-4a6c-b939-57cd91f3bcba"
      }
    },
    "owner": "mostafa7amdy"
  }
}
